{
  "name": "@devicai/ui",
  "version": "0.10.2",
  "type": "module",
  "description": "React component library for Devic AI assistants",
  "engines": {
    "node": ">=20.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    },
    "./styles.css": "./dist/esm/styles.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "typecheck": "tsc --noEmit",
    "yalc:publish": "npm run build && yalc publish --push",
    "yalc:watch": "npm run build && yalc publish && npm run dev"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0",
    "markdown-to-jsx": ">=7.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-postcss": "^4.0.0",
    "tslib": "^2.6.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "react",
    "devic",
    "ai",
    "chat",
    "assistant",
    "components"
  ],
  "author": "Devic <hello@devic.ai>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devicai/devicui.git"
  },
  "homepage": "https://github.com/devicai/devicui#readme",
  "bugs": {
    "url": "https://github.com/devicai/devicui/issues"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "dependencies": {
    "markdown-to-jsx": "^9.6.1"
  }
}
